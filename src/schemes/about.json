{
  "Main": {
    "uid": {
      "type": "UID",
      "config": {
        "label": "uid"
      }
    },
    "banner_text": {
      "type": "Group",
      "config": {
        "fields": {
          "style": {
            "type": "Select",
            "config": {
              "options": [
                "in same line",
                "new line"
              ],
              "default_value": "in same line",
              "label": "style"
            }
          },
          "font_size": {
            "type": "Select",
            "config": {
              "options": [
                "small",
                "big"
              ],
              "default_value": "small",
              "label": "font_size"
            }
          },
          "text_hyperlink_or_space": {
            "type": "Select",
            "config": {
              "options": [
                "text",
                "header",
                "link",
                "space"
              ],
              "default_value": "text",
              "label": "text_hyperlink_or_space"
            }
          },
          "content": {
            "type": "StructuredText",
            "config": {
              "multi": "paragraph",
              "label": "content",
              "placeholder": "hier alle content"
            }
          },
          "type_link": {
            "type": "Select",
            "config": {
              "options": [
                "mailto:",
                "tel:",
                "/",
                "www."
              ],
              "label": "type_link"
            }
          }
        },
        "label": "banner text"
      }
    }
  }
}